{
  "name": "webpack-format-messages",
  "version": "2.0.6",
  "description": "Beautiful formatting for Webpack messages; ported from Create React App!",
  "repository": "lukeed/webpack-format-messages",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "lukeed.com"
  },
  "engines": {
    "node": ">=6"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    }
  },
  "types": "./dist/mjs/index.d.ts",
  "scripts": {
    "build": "rimraf dist/** && tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json"
  },
  "files": [
    "cjs/index.js",
    "mjs/index.js"
  ],
  "keywords": [
    "create-react-app",
    "console",
    "messages",
    "webpack",
    "pretty",
    "format",
    "stats",
    "output"
  ],
  "dependencies": {
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
